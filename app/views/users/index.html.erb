<h1>Users</h1>

<table>
  <thead>
    <tr>
      <th>Email</th>
      <th>Postal_code</th>
      <th>Address</th>
      <th>Self_description</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @users.each do |user| %>
      <tr>
        <td><%= user.email %></td>
        <td><%= user.postal_code %></td>
        <td><%= user.address %></td>
        <td><%= user.self_description %></td>
        <td><%= link_to 'show', user %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<%= paginate @users %>

<br>

<%= link_to 'Back', books_path %>
